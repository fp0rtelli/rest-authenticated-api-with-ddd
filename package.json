{
  "name": "test-georide",
  "version": "1.0.0",
  "author": "Florian Portelli",
  "license": "Florian Portelli",
  "main": "server.js",
  "scripts": {
    "test": "jest --verbose"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ]
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "jest": "26.6.1",
    "node-mocks-http": "1.9.0",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "dependencies": {
    "base-64": "1.0.0",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "uuid": "8.3.1"
  }
}
